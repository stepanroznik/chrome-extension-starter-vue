{
  "name": "chrome-extension-starter",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "prepare": "husky install",
    "affected:lint": "nx affected:lint",
    "start:dev": "nx run-many --target=build --projects=extension-* --configuration development --watch --parallel 5",
    "start:prod": "nx run-many --target=build --projects=extension-* --configuration production --watch --parallel 5",
    "build:dev": "nx run-many --target=build --projects=extension-* --configuration development --parallel 5",
    "build:prod": "nx run-many --target=build --projects=extension-* --configuration production --parallel 5"
  },
  "private": true,
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "postcss": "^8.4.31",
    "rxjs": "~7.8.0",
    "tailwindcss": "^3.3.1",
    "vite": "4.5.1",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-html": "^3.1.0",
    "vue": "^3.3.8",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nx/eslint-plugin": "16.9.1",
    "@nx/jest": "16.9.1",
    "@nx/js": "16.9.1",
    "@nx/linter": "16.9.1",
    "@nx/vite": "16.9.1",
    "@nx/web": "16.9.1",
    "@nx/webpack": "16.9.1",
    "@nx/workspace": "16.9.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/ui": "~0.32.0",
    "commitizen": "^4.3.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^14.0.1",
    "nx": "16.9.1",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-rational-order": "^1.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "vitest": "~0.32.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
